# {{ ansible_managed }}

# The address on the local server to listen to.
KUBE_API_ADDRESS="--address={{kubernetes.kube_api_address}}"

# The port on the local server to listen on.
KUBE_API_PORT="--port={{kubernetes.kube_api_port}}"

# Port kubelets listen on
KUBELET_PORT="--kubelet-port={{kubernetes.kubelet_port}}"

# Address range to use for services
KUBE_SERVICE_ADDRESSES="--service-cluster-ip-range={{kubernetes.kube_service_addresses}}"

# Add your own!
KUBE_API_ARGS="{{kubernetes.kube_api_args}}"
