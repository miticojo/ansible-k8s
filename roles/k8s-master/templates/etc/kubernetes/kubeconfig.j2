apiVersion: v1
clusters:
- cluster:
    certificate-authority-data: {{ cert_ca_crt }}
    server: {{kube_master_api_uri}}
  name: local
contexts:
- context:
    cluster: local
    user: kubelet
  name: service-account-context
current-context: service-account-context
kind: Config
preferences: {}
users:
- name: kubelet
  user:
    client-certificate-data: {{ cert_admin_crt }}
    client-key-data: {{ cert_admin_key }}
